#include <stdio.h>
/**
 *main - Entry point
 *
 *Description: This program prints the alphabet
 *in lowercase, followed by a new line, except q and e.
 *Return: 0 upon successful completion
 */
int main(void)
{
char letter = 'a';

while (letter <= 'z')
{
if (letter == 'e' || letter == 'q')
letter++;
else
{	  putchar(letter);
letter++;
}
}
putchar('\n');
return (0);
}
